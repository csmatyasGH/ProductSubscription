<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>TermNetValue__c</fullName>
    <description>truncated or  Contingent full term NetValues. Will be the source for ARR/MMR</description>
    <externalId>false</externalId>
    <formula>QuoteItem_NetTotal__c * ( Day_Duration__c /  InitialTermDuration__c )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Calculated field based on Qoutel LineItem&#39;s value</inlineHelpText>
    <label>TermNetValue(TSV)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
